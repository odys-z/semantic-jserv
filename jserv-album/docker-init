# WARNING - not tested yet!

docker volume rm album.mysql
docker volume create album.mysql

# for windows (Engine v19.03): cp -r volume/* //wsl$/docker-desktop-data/version-pack-data/community/docker/volumes/album.mysql/_data
# https://stackoverflow.com/a/64418064else
sudo cp -r volume/* /var/lib/docker/volumes/album.mysql/_data

docker run --name jserv-album -v album.mysql:/var/local/volume -p 8080:8080 -d --rm odysz/jserv-album
